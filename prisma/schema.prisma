datasource postgres {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

generator typegraphql {
    provider           = "typegraphql-prisma"
    output             = "../prisma/generated/type-graphql"
    emitTranspiledCode = true
}

model Campground {
    id          String @id @default(uuid())
    name        String
    description String
    imageURL    String
    address     String
    price       Float

    @@map("campgrounds")
}
